{
  "name": "communities-of-practice",
  "version": "0.20.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start:stubs": "nodemon stub-server/server.js",
    "build": "react-scripts build",
    "deploy": "aws s3 sync build/ s3://communites-of-practice --acl public-read",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "format": "prettier 'src/**/*.js' 'src/**/*.jsx'",
    "format:fix": "prettier --write 'src/**/*.js' 'src/**/*.jsx'",
    "lint": "eslint --ext=js --ext=jsx .",
    "lint:fix": "eslint --ext=js --ext=jsx --fix .",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "snyk:test": "snyk test",
    "snyk:wizard": "snyk wizard",
    "snyk:auth": "snyk auth $apiToken",
    "getPackageVersion": "echo $npm_package_version"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.53",
    "apisauce": "^1.1.1",
    "install": "^0.13.0",
    "npm": "^6.14.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cypress": "^4.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint-plugin-cypress": "^2.11.1",
    "express": "^4.17.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.3",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "slug": "^2.1.1",
    "snyk": "^1.1150.0",
    "typescript": "^3.9.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npm run lint:fix",
      "npm run format:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended"
    ],
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  }
}
